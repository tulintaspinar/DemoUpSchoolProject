@model List<DemoUpSchoolProject.Models.Entities.TBL_Testimonial>
<section class="w3l-clients" id="testimonials">
    <div class="midd-w3 py-5">
        <div class="container py-lg-5 py-md-3">
            <div class="row">
                <div class="col-lg-6 align-self">
                    <div class="position-relative px-lg-5">
                        <img src="~/Template/iportfolio-starter/assets/images/clients.png" alt="" class="radius-image img-fluid">
                    </div>
                </div>
                <div class="col-lg-6 mt-lg-0 mt-5">
                    <span class="text">Referanslarım</span>
                    <div id="owl-demo1" class="owl-carousel owl-theme mt-4 py-2 mb-4">
                        @foreach (var item in Model)
                        {
                            <div class="item">
                                <div class="testimonial-content">
                                    <div class="testimonial">
                                        <div class="testi-des">
                                            
                                            <div class="peopl align-self">
                                                <h3>@item.NameSurname</h3>
                                                <div><p>açıklama</p></div>
                                                <p>mail</p>
                                                <p class="indentity">@item.City</p>
                                            </div>
                                        </div>

                                    </div>
                                </div>
                            </div>
                        }

                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
