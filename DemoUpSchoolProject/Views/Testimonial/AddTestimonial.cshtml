@model DemoUpSchoolProject.Models.Entities.TBL_Testimonial
@{
    ViewBag.Title = "AddTestimonial";
    Layout = "~/Views/Shared/_NewAdminLayout.cshtml";
}

<h2>REFERANS EKLE</h2>

<form method="post">
    <label>Ad Soyad</label>
    @Html.TextBoxFor(x => x.NameSurname, new { @class = "form-control" })
    <br />
    <label>ŞEHİR</label>
    @Html.TextBoxFor(x => x.City, new { @class = "form-control" })
    <br />
    <label>MESLEK</label>
    @Html.TextBoxFor(x => x.Profession, new { @class = "form-control" })
    <br />
    <label>Açıklama</label>
    @Html.TextBoxFor(x => x.Description, new { @class = "form-control" })
    <br />
    <button class="btn btn-info">KAYDET</button>
</form>
